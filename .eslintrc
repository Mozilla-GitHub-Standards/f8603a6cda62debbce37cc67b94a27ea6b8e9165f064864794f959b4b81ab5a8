{
  "extends": [
    "airbnb",
  ],
  "rules": {
    "indent": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "func-names": 0,
    "comma-dangle": [2, "never"],
    "object-curly-spacing": [2, "never"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "react/no-multi-comp": 0
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "__CONFIG__": false
  }
}
